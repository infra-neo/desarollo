# Example Terraform Variables
# Copy this file to terraform.tfvars and fill in your values

project_id       = "your-gcp-project-id"
region           = "us-central1"
zone             = "us-central1-a"
instance_name    = "desarollo-vm"
machine_type     = "e2-medium"
disk_size_gb     = 50
network_name     = "desarollo-vpc"
subnet_name      = "desarollo-subnet"
subnet_cidr      = "10.0.0.0/24"
static_ip_name   = "desarollo-static-ip"
network_tags     = ["desarollo-vm", "http-server", "https-server"]
